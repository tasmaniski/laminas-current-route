{
    "name": "tasmaniski/laminas-current-route",
    "description": "View Helper for reading current route info: Controller, Action, Module name",
    "type": "library",
    "keywords": [
        "laminas framework",
        "view",
        "helper",
        "route"
    ],
    "license": "MIT",
    "homepage": "https://github.com/tasmaniski",
    "authors": [
        {
            "name": "Aleksandar Varnicic",
            "email": "tasmaniski@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "laminas/laminas-mvc": "^3.0",
        "laminas/laminas-dependency-plugin": "^1.0 | ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0"
    },
    "autoload": {
        "psr-0": {
            "CurrentRoute": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "psr-0": {
            "CurrentRouteTest": "test/"
        }
    }
}
